program nome2;
{exemplo 2} 
var a: real; 
var b: integer; 
procedure nomep (x: real);
var a, c: integer; 
begin 
readln(c, a); 
if a<18 then 
begin 
a:= c+x; 
writeln(a); 
end 
else c:= a+x; 
end; 
begin {programa principal} 
readln(b); 
nomep(b); 
while b<10 do 
begin 
b:=b+2; 
a:=a-1; 
end;
while a> 10 do 
begin 
b:=b+2; 
a:=a-1; 
end;
end.